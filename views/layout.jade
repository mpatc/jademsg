doctype html
html(lang="en")
  head
    title= title
    link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css")
    link(rel="stylesheet", href="animate.css")
    link(rel="stylesheet", href="style.css")
    script(src="http://code.jquery.com/jquery-2.2.3.min.js")
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js")
    script(src='main.js')
    
body
  nav.navbar.navbar-default.container-fluid
    .card.center-block.col-md-10
      .navbar-header.center-block
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='#')= title
      #bs-example-navbar-collapse-1.collapse.navbar-collapse
        ul.nav.navbar-nav
          li
            a(href='/contact') Contact
          li
            a(href='/other') Other
          li
            a(href='/') Home
            form.navbar-form.navbar-left(role='search')
      .form-group
        input#msg.form-control(type='text', placeholder='msg')
      .form-group
        input#name.form-control(type='text', placeholder='name')
      button#namer.btn.btn-default(type='submit') Submit


  block content
